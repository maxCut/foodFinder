{
  "name": "FoodFinder",
  "description": "getting some grub",
  "version": "1.1",
  "manifest_version": 3,
  "content_scripts":
  [
      {
        "matches": [
          "http://*/*",
          "https://*/*"],
        "js" : ["content-script.js"]
      }
  ],
  "externally_connectable": {
    "ids": ["*"],
    "matches": ["http://my.localhost/*"]
  },
  "permissions": ["alarms","storage"]
}
